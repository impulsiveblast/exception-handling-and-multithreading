class A implements Runnable{
    
    public void run(){
        
        for(int i=0;i<5;i++)
        {
          System.out.println("4th year");    
        }
        
    }
}


class B{

    public static void main(String[] args){
        
        A a1=new A();
        Thread t1=new Thread(a1);
        
        
        t1.start();
        
    }
}
