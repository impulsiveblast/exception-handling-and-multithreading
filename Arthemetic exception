public class Exception {
    public static void main(String[] args) {
        // System.out.println("Hello World");

        int a, b, c = 0;
        a = 0;
        b = 8;

        try {
            c = b / a;
        } 
        catch (ArithmeticException e) 
        {
            System.out.println("Any message u want to print : " + e);
        }

        System.out.println(c);
    }
}
